#!/bin/bash

# Variabel dan warna ANSI
e="echo -e"
s="sleep 1"
ss="sleep 2"
sss="sleep 3"

m="\033[1;31m"   # Merah
h="\033[1;32m"   # Hijau
k="\033[1;33m"   # Kuning
b="\033[1;34m"   # Biru
u="\033[1;35m"   # Ungu
pu="\033[1;30m"  # Abu-abu
res="\033[0m"    # Reset

# Persiapan
clear
termux-setup-storage -y
apt install -y mpg123 jq telegram
clear

# Animasi loading
loading_animation() {
    bar=""
    for ((i=1; i<=50; i++)); do
        color="\033[1;3$((i % 7))m" # Warna secara dinamis (31-37)
        bar+="‚£ø"
        echo -ne "${color}${bar}${res}\r"
        sleep 0.1
    done
    echo -e "${res}\nLoading selesai!"
}

$e "${u}MOHON DI TUNGGU SAYANG üíÉ${res}"
loading_animation

clear

# Banner (gunakan file eksternal jika terlalu panjang)
$e "${u}Selamat datang di script!${res}"


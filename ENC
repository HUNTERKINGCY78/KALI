const { default: makeWASocket, useMultiFileAuthState, fetchLatestBaileysVersion } = require("@whiskeysockets/baileys");
const pino = require('pino');

(async () => {
    const { state, saveState } = await useMultiFileAuthState('./session');
    const { version, isLatest } = await fetchLatestBaileysVersion();
    const bot = makeWASocket({
        logger: pino({ level: 'silent' }),
        auth: state,
        browser: ["Ubuntu", "Chrome", "20.04"]
    });

    bot.ev.on('connection.update', (update) => {
        const { connection, lastDisconnect, qr } = update;

        if (connection === 'close') {
            console.log('Connection closed due to', lastDisconnect.error);
        } else if (connection === 'open') {
            console.log('Connected!');
        }

        // Menampilkan QR code untuk pemindaian
        if (qr) {
            console.log('Scan the following QR code:');
            console.log(qr);
        }
    });

    // Kirim pesan spam
    for (let i = 0; i < ${spam_count}; i++) {
        try {
            console.log("Spam ke ${phone_number}, Total: ", i + 1);
            // Kirim pesan logika (tambahkan implementasi spesifik Anda di sini)
        } catch (error) {
            console.error("Error:", error.message);
        }
    }

    // Simpan state setelah selesai
    await saveState();
})();
EOF

